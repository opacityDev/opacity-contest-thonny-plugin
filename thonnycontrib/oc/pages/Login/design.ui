<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="oc_login" named="True">
    <property name="height">300</property>
    <property name="resizable">none</property>
    <property name="width">600</property>
    <containerlayout manager="pack">
      <property name="propagate">False</property>
    </containerlayout>
    <child>
      <object class="ttk.Label" id="label2">
        <property name="font">{Roboto Mono} 24 {}</property>
        <property name="text" translatable="yes">Opacity Contests</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label3">
        <property name="font">{Roboto Mono} 20 {}</property>
        <property name="text" translatable="yes">Sign in</property>
        <layout manager="pack">
          <property name="pady">10</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label5">
        <property name="font">{Roboto Mono} 12 {}</property>
        <property name="text" translatable="yes">Welcome to Opacity Contests portal on Thonny</property>
        <layout manager="pack">
          <property name="pady">20</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame3">
        <property name="height">20</property>
        <property name="width">270</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
        <containerlayout manager="pack">
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="label7">
            <property name="text" translatable="yes">Username :</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="width">80</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="username" named="True">
            <layout manager="place">
              <property name="width">180</property>
              <property name="x">90</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame4">
        <property name="height">20</property>
        <property name="width">270</property>
        <layout manager="pack">
          <property name="pady">20</property>
          <property name="side">top</property>
        </layout>
        <containerlayout manager="pack">
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="label8">
            <property name="text" translatable="yes">Password :</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="width">80</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="password" named="True">
            <layout manager="place">
              <property name="width">180</property>
              <property name="x">90</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="login_btn" named="True">
        <property name="text" translatable="yes">Login</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
